{
  "name": "qwik-router",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/qwik-router/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/qwik-router",
        "configFile": "packages/qwik-router/vite.config.ts",
        "mode": "lib"
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs qwik-router {args.ver} {args.tag}",
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "../../coverage/packages/qwik-router"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/qwik-router"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/qwik-router/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
